{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-console": "warn",
    "semi": ["warn", "always"],
    "import/order": [
      "error",
      {
        "groups": [
          "index",
          "sibling",
          "parent",
          "internal",
          "external",
          "builtin",
          "object",
          "type"
        ]
      }
    ]
  }
}
